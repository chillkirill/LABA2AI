    "import numpy as np\n",
    "\n",
    "# Создание массив NumPy 3x3 от 1 до 9\n",
    "arr = np.arange(1, 10).reshape((3, 3))\n",
    "\n",
    "arr = arr * 2\n",
    "\n",
    "# Заменяем все элементы больше 10 на 0\n",
    "arr[arr > 10] = 0\n",
    "\n",
    "print(arr)\n"
   ]
